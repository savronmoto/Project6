				; ---------------------------------------------------------------------------------
				; Name: writeVal
				;	Convert a numeric SDWORD value (input parameter, by value) to a string of ascii digits
				; Postconditions:
				; Receives;	    	[EBP+8] = validInt - input parameter, by value - SDWORD
				; Returns: 
				; ---------------------------------------------------------------------------------
 00000110			writeVal PROC
				; preserve EBP
 00000110  55			  push	EBP				
 00000111  8B EC		  mov	EBP, ESP
				; preserve registers
 00000113  60			  pushad

				; If the SDWORD is negative, first add the ascii character for -, whichi is 45 (2Dh) ***********DO NEG PART*********

				; then for each digit, divide by 10. the remainder plus 48 is the ascii code for the digit.
 00000114  BB 0000000A		  mov  EBX, 10
 00000119  8B 45 08		  mov  EAX, [EBP+8]  ; move validInt into dividend
 0000011C  B9 00000000		  mov  ECX, 0		 ; counter
 00000121			_divLoop:
 00000121  99			  cdq
 00000122  F7 FB		  idiv EBX			 ; EAX/EBX = q: EAX r: EDX
 00000124  83 C2 30		  add  EDX, 48
 00000127  52			  push EDX
 00000128  41			  inc  ECX
 00000129  83 F8 00		  cmp  EAX, 0		 ; if quotient is 0 we can stop
 0000012C  75 F3		  jnz  _divLoop		 ; then do it again. divide the quotient by 10, and the remainder plus 48 is the second to last digit. and so on.
 0000012E			_writeLoop:
 0000012E  5B			  pop  EBX
				  mDisplayString EBX ; Invoke the mDisplayString macro to print the ascii representation of the SDWORD value to the output.
 0000012F  52		     1	  push  EDX
 00000130  8B D3	     1	  mov	EDX, EBX
 00000132  E8 00000000 E     1	  call	WriteString
 00000137  E8 00000000 E     1	  call  CrLf
 0000013C  5A		     1	  pop	EDX
 0000013D  E2 EF		  LOOP _writeLoop

 0000013F  C2 0004		  ret  4
 00000142			writeVal ENDP

